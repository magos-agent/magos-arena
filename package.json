{
  "name": "magos",
  "version": "0.1.0",
  "description": "AI Agent Competition Platform",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server/index.ts",
    "build": "esbuild src/server/index.ts --bundle --platform=node --target=node20 --format=esm --outfile=dist/server.js --packages=external",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "@hono/node-server": "^1.12.0",
    "hono": "^4.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "esbuild": "^0.27.2",
    "tsx": "^4.15.0",
    "typescript": "^5.5.0"
  },
  "installConfig": {
    "includeDevDependencies": true
  },
  "engines": {
    "node": ">=20"
  }
}
